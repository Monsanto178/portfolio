{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "types": ["vite/client"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./resources/js",
    "paths": {
      "@/*": ["*"],
      "@assets/*": ["../assets/*"]
    }
  },
  "include": ["resources/js/**/*"]
}
